{
  "name": "obra360-dashboard",
  "version": "1.0.0",
  "description": "Dashboard modular para Obra 360 - Sistema de gestión de construcción",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "webpack --mode=production",
    "watch": "webpack --mode=development --watch",
    "setup": "node setup-windows.js",
    "setup-unix": "node setup.js",
    "clean": "node -e \"const fs=require('fs'); ['dist','logs','uploads','reports'].forEach(d=>{try{fs.rmSync(d,{recursive:true,force:true})}catch(e){}})\"",
    "lint": "eslint . --ext .js",
    "test": "echo \"Tests coming soon...\" && exit 0"
  },
  "keywords": [
    "dashboard",
    "construccion",
    "obra360",
    "management",
    "express",
    "ejs",
    "bootstrap"
  ],
  "author": "Obra 360 Team",
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-static": "^1.2.6",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.45.0",
    "nodemon": "^3.0.1",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/obra360-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/obra360-dashboard/issues"
  },
  "homepage": "https://github.com/tu-usuario/obra360-dashboard#readme"
}
